<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pe.gob.trabajo.pcd.modelo.dominio.EmpleadoMaestro" table="SITB_PROFESION">
        <id name="nombres" type="string">
            <column name="V_CODPROF" scale="0" />
        </id>        
        <property name="descripcion" type="string">
            <column name="V_DESPROF" length="100" />
        </property>
    </class>
</hibernate-mapping>
