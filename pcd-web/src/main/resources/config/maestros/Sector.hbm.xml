<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pe.gob.trabajo.pcd.modelo.dominio.Sector" table="SITB_SECTOR">
		<id name="id" type="java.lang.Long">
			<column name="N_CODSEC" scale="0" />
		</id>
		<property name="descripcion" type="string">
			<column name="V_DESSEC" length="150" />
		</property>
		<property name="flgAct" type="string">
			<column name="V_FLGACT" length="1" />
		</property>
		<property name="nivel" type="java.lang.Long">
			<column name="N_CODNIVEL" scale="0" />
		</property>		
		
	</class>
</hibernate-mapping>
