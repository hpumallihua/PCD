<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pe.gob.trabajo.pcd.modelo.dominio.Nacionalidad"
		table="SITB_NACIONALIDAD">
		<id name="id" type="string">
			<column name="V_CODNACION" length="6" />
		</id>
		<property name="descripcion" type="string">
			<column name="V_DESNACION" length="300" />
		</property>
		<property name="flgsunmin" type="java.lang.Integer">
			<column name="N_FLGSUNMIN" scale="0" />
		</property>
	</class>
</hibernate-mapping>
